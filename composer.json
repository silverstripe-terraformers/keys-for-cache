{
    "name": "silverstripe-terraformers/keys-for-cache",
    "description": "Silverstripe cache key management",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cache",
        "keys"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/framework": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "tractorcow/silverstripe-fluent": ">=4.0.0",
        "slevomat/coding-standard": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Terraformers\\KeysForCache\\": "src/",
            "Terraformers\\KeysForCache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
